{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
    
        {
            "type": "node",
            "request": "launch",
            "name": "CLI: Init Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\test-init",
            "program": "${workspaceFolder}\\src\\cli\\index.ts",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "args": ["init", "c4"],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/dist/**/*.js",
                "${workspaceFolder}/node_modules/trellisuml/**/*.js",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CLI: Build Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\test-init",
            "program": "${workspaceFolder}\\src\\cli\\index.ts",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "args": ["build"],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/dist/**/*.js",
                "${workspaceFolder}/node_modules/trellisuml/**/*.js",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CLI: Watch Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\test-init",
            "program": "${workspaceFolder}\\src\\cli\\index.ts",
            "console": "integratedTerminal",
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "args": ["watch"],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/dist/**/*.js",
                "${workspaceFolder}/node_modules/trellisuml/**/*.js",
            ]
        }
    ]
}